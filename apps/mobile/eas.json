{
  "cli": {
    "appVersionSource": "remote",
    "version": "^13.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_EXPO_ENV": "development"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_EXPO_ENV": "preview"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_EXPO_ENV": "production"
      }
    }
  }
}
